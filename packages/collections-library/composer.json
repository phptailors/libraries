{
    "name": "phptailors/collections-library",
    "description": "Objects and functions operating on collections",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Pawe≈Ç Tomulik",
            "email": "pawel@tomulik.pl"
        }
    ],
    "support": {
      "issues": "https://github.com/phptailors/libraries/issues",
      "source": "https://github.com/phptailors/libraries"
    },
    "conflict": {
        "nikic/php-parser": "< 4.13.2"
    },
    "require": {
        "php": "^8.1",
        "phptailors/collections-interface": "*"
    },
    "provide": {
        "phptailors/collections-implementation": "*"
    },
    "require-dev": {
        "phptailors/phpunit-inheritance": "dev-master",
        "phptailors/phpunit-arrays": "dev-master",
        "phpunit/phpunit": "^9.5.10 || ^10"
    },
    "autoload": {
      "psr-4" : {
        "Tailors\\Lib\\Collections\\" : "src/"
      },
      "exclude-from-classmap": [
        "/tests/"
      ]
    },
    "autoload-dev": {
      "psr-4" : {
        "Tailors\\Lib\\Collections\\" : "tests/src/"
      }
    }
}
